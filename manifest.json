{
    "name": "MADS_ResD",
    "version": "1.0",
    "description": "Downloads resources from coursera",
    "permissions": ["storage", "downloads", "identity"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "page_action": {
      "default_popup": "popup.html"
    },
    "default_icon": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
    },
    "content_scripts": [
      {
        "matches": [
          "https://*.coursera.org/learn/*/resources*"
        ],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    },
    "manifest_version": 2
}